<!DOCTYPE html>
<html class="iphorm-js" dir="ltr" lang="en-US">
<head>
    <meta http-equiv="content-type" content="text/html; charset=UTF-8">
    <meta charset="utf-8">
    <title>@ViewBag.Title</title>
    <link rel="stylesheet" href="@Url.Content("~/Content/admin/css/style.css")" type="text/css" />
    <link rel="stylesheet" href="@Url.Content("~/Content/admin/css/custom.css")" type="text/css" />
    <link rel="stylesheet" href="@Url.Content("~/Content/admin/css/print.css")" type="text/css" media="print" />
    <link href="@Url.Content("~/Content/style_new.css")" rel="stylesheet" type="text/css">
    <link rel="stylesheet" href="@Url.Content("~/Content/skeleton.css")" />
    <link rel="stylesheet" href="@Url.Content("~/Content/style-custom_new.css")" type="text/css" media="all" />

    <script src="@Url.Content("~/Scripts/jquery-3.3.1.min.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/jquery.validate.min.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/jquery.validate.unobtrusive.min.js")" type="text/javascript"></script>

    <script src="@Url.Content("~/Scripts/jquery.maskedinput-1.3.min.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/trippleddlAdapters.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/customForUserPage.js")" type="text/javascript"></script>
    @RenderSection("scripts", required: false)
</head>
<body>
    <!-- Header -->
    <header id="top">
        <div class="wrapper-login">
            <!-- Title/Logo - can use text instead of image -->
            <div id="title"><img src="@Url.Content("~/Content/admin/img/logo_small.png")" alt="Egghead Academy" /><!--<span>Administry</span> demo--></div>
            <!-- Main navigation -->
            <nav id="menu">
                <ul class="sf-menu"></ul>
            </nav>
            <!-- End of Main navigation -->
            <!-- Aside links -->
            @*<aside><b>English</b> &middot; <a href="#">Spanish</a> &middot; <a href="#">German</a></aside>*@
            <!-- End of Aside links -->
        </div>
    </header>
    <!-- End of Header -->
    <!-- Page title -->
    <div id="pagetitle">
        <div class="wrapper">
        </div>
    </div>
    <!-- End of Page title -->
    <!-- Page content -->
    <div id="page">
        <!-- Wrapper -->
        <div class="wrapper" style="margin-top:10px;">
            @RenderBody()
            <!-- End of login form -->
        </div>
        <!-- End of Page content -->
        <!-- User interface javascript load -->
</body>
</html>
