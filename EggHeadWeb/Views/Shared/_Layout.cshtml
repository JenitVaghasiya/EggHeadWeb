<!DOCTYPE html>
<html class="iphorm-js" dir="ltr" lang="en-US">
<head>
    <meta http-equiv="content-type" content="text/html; charset=UTF-8">
    <meta charset="utf-8">
    <title>@ViewBag.Title</title>
    <link rel="stylesheet" href="@Url.Content("~/Content/style.css")" type="text/css" />
    <link rel="stylesheet" href="@Url.Content("~/Content/user_style1.css")" type="text/css" />
    <link rel="stylesheet" href="@Url.Content("~/Content/skeleton.css")" />
    <link rel="stylesheet" href="@Url.Content("~/Content/layout.css")" />
    <link rel="stylesheet" id="style-custom-css" href="@Url.Content("~/Content/style-custom.css")" type="text/css" media="all" />
    <link rel="stylesheet" id="Google-Font-css" href="@Url.Content("~/Content/css.css")" type="text/css" media="all" />
    <link rel="stylesheet" id="iphorm-css" href="@Url.Content("~/Content/styles.css")" type="text/css" media="all" />
    <link rel="stylesheet" id="qtip-css" href="@Url.Content("~/Content/jquery_002.css")" type="text/css" media="all" />
    <link rel="stylesheet" id="iphorm-fancybox-css" href="@Url.Content("~/Content/jquery.css")" type="text/css" media="all" />
    <link rel="stylesheet" id="iphorm-uniform-theme-1-css" href="@Url.Content("~/Content/default.css")" type="text/css" media="all" />
    <link rel="stylesheet" id="iphorm-custom-css" href="@Url.Content("~/Content/custom.css")" type="text/css" media="all" />
    <link rel="stylesheet" id="superfish-css" href="@Url.Content("~/Content/superfish.css")" type="text/css" media="all" />
    <link rel="stylesheet" id="prettyPhoto-css" href="@Url.Content("~/Content/prettyPhoto.css")" type="text/css" media="all" />
    <script src="@Url.Content("~/Scripts/jquery-1.5.1.min.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/jquery.validate.min.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/jquery.validate.unobtrusive.min.js")" type="text/javascript"></script>
</head>
<body>
    <div class="body-wrapper">
        <div class="top-navigation container">
            <div class="top-navigation-wrapper">
                <div class="top-navigation-left">
                    <div class="menu-top-menu-container">
                        <ul id="menu-top-menu" class="menu">
                            <li id="menu-item-35" class="menu-item menu-item-type-post_type menu-item-object-page current-menu-item page_item page-item-21 current_page_item menu-item-35">
                                <a href="http://professoreggheadscienceacademy.com/">Home</a></li>
                            <li id="menu-item-656" class="menu-item menu-item-type-post_type menu-item-object-page menu-item-656">
                                <a href="http://professoreggheadscienceacademy.com/science-academy/employment/">Employment</a></li>
                            <li id="menu-item-34" class="menu-item menu-item-type-post_type menu-item-object-page menu-item-34">
                                <a href="http://professoreggheadscienceacademy.com/contact/">Contact</a></li>
                        </ul>
                    </div>
                    <br class="clear">
                </div>
            </div>
        </div>
        <div class="container">
            <div class="header-wrapper">
                <div class="inner-header-wrapper">
                    <!-- Get Logo -->
                    <div class="logo-wrapper">
                        <a href="http://professoreggheadscienceacademy.com/">
                            <img src="http://professoreggheadscienceacademy.com/wp-content/uploads/2012/05/logo_small-e1337319549368.png"
                                alt="logo" /></a>
                    </div>
                    <!-- Get Search -->
                    <div class="search-wrapper">
                        <form method="get" id="searchform" action="http://professoreggheadscienceacademy.com/">
                        <div id="search-text">
                            <input type="text" value="Type your keywords..." name="s" id="s" autocomplete="off"
                                data-default="Type your keywords..." />
                        </div>
                        <input type="submit" id="searchsubmit" value="Search" />
                        <br class="clear" />
                        </form>
                    </div>
                    <br class="clear" />
                </div>
                @* Inner header wrapper *@ @* Navigation *@
                <div class="sixteen columns mb0">
                    <div class="navigation-wrapper">
                        @* Get Navigation *@
                        <div id="main-superfish-wrapper" class="menu-wrapper">
                            <ul id="menu-main-menu" class="sf-menu">
                                <li id="menu-item-28" class="menu-item menu-item-type-post_type menu-item-object-page menu-item-28">
                                    <a href="http://professoreggheadscienceacademy.com/">Home</a></li>
                                <li id="menu-item-615" class="menu-item menu-item-type-post_type menu-item-object-page menu-item-615">
                                    <a href="http://professoreggheadscienceacademy.com/after-school-enrichment-classes/">
                                        Classes</a></li>
                                <li id="menu-item-613" class="menu-item menu-item-type-post_type menu-item-object-page menu-item-613">
                                    <a href="http://professoreggheadscienceacademy.com/science-camp/">Science Camp</a></li>
                                <li id="menu-item-614" class="menu-item menu-item-type-post_type menu-item-object-page menu-item-614">
                                    <a href="http://professoreggheadscienceacademy.com/science-birthday/">Parties</a></li>
                                <li id="menu-item-616" class="menu-item menu-item-type-post_type menu-item-object-page menu-item-616">
                                    <a href="http://professoreggheadscienceacademy.com/science-workshops/">Workshops</a></li>
                                <li id="menu-item-933" class="menu-item menu-item-type-post_type menu-item-object-page menu-item-933">
                                    <a href="http://professoreggheadscienceacademy.com/science-academy/">Science Academy</a></li>
                                <li id="menu-item-27" class="menu-item menu-item-type-post_type menu-item-object-page menu-item-27">
                                    <a href="http://professoreggheadscienceacademy.com/contact/">Contact</a></li>
                            </ul>
                        </div>
                        <div class="social-wrapper">
                            <div class="social-icon-wrapper">
                                <div class="social-icon">
                                    <a href="http://www.facebook.com/professoregghead">
                                        <img src="@Url.Content("~/Content/images/facebook.png")" alt="facebook" /></a></div>
                                <div class="social-icon">
                                    <a href="http://twitter.com/profegghead">
                                        <img src="@Url.Content("~/Content/images/twitter.png")" alt="twitter" /></a></div>
                            </div>
                        </div>
                        <br class="clear" />
                    </div>
                </div>
                <br class="clear">
            </div>
            @* header-wrapper *@
            <div class="content-wrapper">
                <div id="page-content">
                @RenderBody()
                </div>
            </div>
            @* content-wrapper *@
            <div class="clear">
            </div>
            <div class="content-bottom-gimmick">
            </div>
        </div>
        @* container *@
        <div class="footer-wrapper">
            <div class="footer-blank-space">
            </div>
            <div class="container mt0">
                @* Get Footer Widget *@
                <div class="footer-widget-wrapper">
                    <div class="eight columns mb0">
                        <div class="custom-sidebar gdl-divider">
                            <div class="textwidget">
                                <p>
                                    <img src="@Url.Content("~/Content/images/logo_small_lt.png")" /></p>
                            </div>
                        </div>
                    </div>
                    <div class="four columns mb0">
                        <div class="custom-sidebar gdl-divider">
                            <h3 class="custom-sidebar-title footer-title-color gdl-title">
                                Contact Info</h3>
                            <div class="textwidget">
                                <p>
                                    Tel: 310.500.8187<br />
                                    Fax: 310.464.3535<br />
                                    Info@ACEenrichment.com</p>
                                <p>
                                    Mailing Address:<br />
                                    7162 Beverly Blvd Ste 331<br />
                                    Los Angeles, CA 90036</p>
                            </div>
                        </div>
                    </div>
                    <div class="four columns mb0">
                        <div class="custom-sidebar gdl-divider">
                            <h3 class="custom-sidebar-title footer-title-color gdl-title">
                                Science Academy Services</h3>
                            <div class="menu-science-academy-services-container">
                                <ul id="menu-science-academy-services" class="menu">
                                    <li id="menu-item-757" class="menu-item menu-item-type-post_type menu-item-object-page menu-item-757">
                                        <a href="http://professoreggheadscienceacademy.com/after-school-enrichment-classes/">
                                            Classes</a></li>
                                    <li id="menu-item-758" class="menu-item menu-item-type-post_type menu-item-object-page menu-item-758">
                                        <a href="http://professoreggheadscienceacademy.com/science-birthday/">Parties</a></li>
                                    <li id="menu-item-759" class="menu-item menu-item-type-post_type menu-item-object-page menu-item-759">
                                        <a href="http://professoreggheadscienceacademy.com/science-camp/">Science Camp</a></li>
                                    <li id="menu-item-760" class="menu-item menu-item-type-post_type menu-item-object-page menu-item-760">
                                        <a href="http://professoreggheadscienceacademy.com/science-workshops/">Workshops</a></li>
                                </ul>
                            </div>
                        </div>
                    </div>
                    <div class="display-none mb0">
                    </div>
                    <br class="clear">
                </div>
                @* Get Copyright Text *@
                <div class="copyright-wrapper gdl-divider">
                    <div class="copyright-left">
                        2012 Copyright Activities Children Enjoy, LLC. All Rigth Reserved.
                    </div>
                    <div class="copyright-right">
                        Professor Egghead Science Academy
                    </div>
                    <div class="clear">
                    </div>
                </div>
            </div>
            @* container-wrapper *@
        </div>
        @* footer-wrapper *@
    </div>
    @* body-wrapper *@
</body>
</html>
